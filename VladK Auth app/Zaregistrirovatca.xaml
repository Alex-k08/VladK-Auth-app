<Page x:Class="VladK_Auth_app.Zaregistrirovatca"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:VladK_Auth_app"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Zaregistrirovatca">

    <StackPanel>
        <Label Content="Login" ></Label>
        <TextBox Grid.Row="1" x:Name="Login"></TextBox>
        <Label Grid.Row="2" Content="Parol"></Label>
        <PasswordBox Grid.Row="3" x:Name="Password"></PasswordBox>
        <Label Grid.Row="4" Content="Povtorite parol"></Label>
        <PasswordBox Grid.Row="5" x:Name="Povtorite"></PasswordBox>
        <Button Grid.Row="6" Content="зарегаться" Click="Button_Click"></Button>
    </StackPanel>
</Page>
